{
  "scripts": {
    "bootstrap": "node ./scripts/bootstrap.js",
    "change": "changeset add",
    "change:version": "changeset version",
    "change:publish": "changeset publish",
    "start": "pnpm run dev",
    "dev": "dumi dev",
    "build": "pnpm run -r build",
    "docs:build": "dumi build",
    "release": "node ./scripts/release.js",
    "prepare": "husky install && dumi setup",
    "doctor": "father doctor",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "pnpm run lint:es && pnpm run lint:css",
    "lint:css": "stylelint \"packages/**/src/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "preinstall": "npx only-allow pnpm"
  },
  "authors": [],
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@firesoon/ant-ui": "^4.0.7",
    "@firesoon/antd": "^1.0.5",
    "@firesoon/icons-react": "^2.1.14",
    "@firesoon/rc-table": "^1.0.12",
    "ahooks": "^3.7.4",
    "classnames": "^2.3.2",
    "lodash": "^4.17.21",
    "react-zmage": "0.8.5-beta.37"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/runtime": "^7.20.13",
    "@changesets/cli": "^2.26.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^18.0.10",
    "@umijs/lint": "^4.0.0",
    "@umijs/utils": "^4.0.47",
    "add": "^2.0.6",
    "animated-scroll-to": "^2.3.0",
    "babel-plugin-import": "^1.13.6",
    "dumi": "^2.1.10",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "enzyme-to-json": "^3.6.2",
    "esbuild": "^0.17.6",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.23.0",
    "execa": "^5.1.1",
    "father": "^4.1.3",
    "husky": "^8.0.1",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.2",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.21",
    "postcss-less": "^6.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "rc-tabs": "^12.5.6",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-is": "18.2.0",
    "styled-components": "^5.3.6",
    "stylelint": "^14.9.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-recess-order": "^3.1.0",
    "stylelint-config-standard": "^29.0.0",
    "typescript": "4.9.4",
    "webpack": "^5.75.0"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  }
}
